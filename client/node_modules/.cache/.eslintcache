._favorites\r\n    }\r\n\r\n    get totalCount() {\r\n        let counter=0\r\n        for(var item of this._goods){\r\n            counter+=item.amount\r\n        }\r\n        return counter;\r\n    }\r\n\r\n    get goods() {\r\n        return this._goods\r\n    }\r\n\r\n    get totalSum() {\r\n        let total = 0\r\n        this._goods.map((m) => {\r\n            total += m.good.price * m.amount\r\n        })\r\n        return total\r\n    }\r\n\r\n    get isEmpty(){\r\n        return this.totalCount==0\r\n    }\r\n}","C:\\Users\\jhokipery\\Documents\\works\\onepro\\onepro\\client\\src\\pages\\Main.js",["258"],[],"import React, {useContext, useEffect} from 'react';\r\nimport MainCategoryList from \"../components/MainPage/MainCategoryList\";\r\nimport MainPartnersSlider from \"../components/MainPage/MainPartnersSlider\";\r\nimport SliderTop from '../components/MainPage/sliderTop'\r\nimport {observer} from 'mobx-react-lite'\r\nimport {Context} from '../index'\r\nimport {fetchCategories, fetchSlider} from '../http/shopAPI'\r\n\r\n\r\n\r\nconst Main = observer(() => {\r\n    const {good} = useContext(Context)\r\n\r\n    useEffect(()=>{\r\n        fetchCategories().then(data=>good.setCategories(data))\r\n        fetchSlider(1).then(data=>good.setMainPageSlider(data))\r\n    },[])\r\n\r\n    return (\r\n            <div className=\"main\">\r\n                <SliderTop/>\r\n                <MainCategoryList/>\r\n\r\n                <div className=\"equipments\">\r\n                    <div className=\"equipments-left\">\r\n                        <h2 className=\"equipments-title\">\r\n                            Универсальный поставщик<br />\r\n                            оборудования и комплектующих\r\n                        </h2>\r\n                        <p>\r\n                            На сегодняшний день компания ISELF является надежным партнером различных бизнес-проектов в сфере\r\n                            моечно-уборочной техники и автохимии. Мы предлага